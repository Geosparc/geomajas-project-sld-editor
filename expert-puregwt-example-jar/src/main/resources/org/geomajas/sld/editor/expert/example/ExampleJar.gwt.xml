<module rename-to="examplejar">
	<inherits name='org.geomajas.gwt2.example.base.ExampleBase' />
	
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name="com.gwtplatform.mvp.Mvp" />
    <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>
    
    <inherits name='org.geomajas.gwt2.widget.GeomajasGwtWidget' />
    
    <inherits name="org.geomajas.sld.editor.expert.GeomajasSldExpertPureGwt"/>  <!-- using pure Gwt -->
    
    <extend-property name="locale" values="nl" />
	<extend-property name="locale" values="en"/>

	<source path='client'/>

 	<!-- This is required for Gwt-Platform Proxy's generator -->       
	<define-configuration-property name="gin.ginjector" is-multi-valued="false" />
	<set-configuration-property name="gin.ginjector" value="org.geomajas.sld.editor.expert.example.client.gin.SldEditorClientGinjector" />
 
    <entry-point class="org.geomajas.sld.editor.expert.example.client.ExampleJar"/>
</module>